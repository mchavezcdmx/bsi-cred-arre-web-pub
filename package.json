{
  "name": "bancoppel-next-base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "nodemon": "nodemon",
    "dev": "npm-run-all --parallel nodemon watch:js watch:css",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node dist/server/index.js",
    "start:win": "set NODE_ENV=production&&node dist/server/index.js",
    "lint": "npm-run-all lint:js lint:css",
    "lint:css": "stylelint components/**/*.scss styles/**/*.scss --syntax scss",
    "lint:css:fix": "stylelint components/**/*.scss styles/**/*.scss --fix",
    "lint:js": "eslint pages/** components/**/*.tsx components/**/*.js server/** helpers/** constants/** redux/** services/**",
    "lint:js:fix": "eslint pages/** components/**/*.tsx components/**/*.js server/** helpers/** constants/** redux/** services/** --fix",
    "test": "jest -u --coverage",
    "test:only": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "watch:css": "onchange 'components/**/*.scss' 'styles/**/*.scss' -- npm-run-all lint:css",
    "watch:js": "onchange 'pages/**/*.js' -- npm-run-all lint:js",
    "precomit": "npm-run-all lint:js lint:css build"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@contentful/rich-text-html-renderer": "^15.0.0",
    "@reduxjs/toolkit": "^1.6.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "bootstrap": "^5.0.1",
    "contentful": "^8.3.7",
    "express": "^4.17.1",
    "interweave": "^12.7.2",
    "inversify": "^5.1.1",
    "next": "10.2.3",
    "next-routes": "^1.4.2",
    "postcss": "^8.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.16.1",
    "react-redux": "^7.2.4",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.34.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/react": "17.0.9",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-jest": "^27.0.2",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "jest-css-modules": "^2.1.0",
    "jest-dom": "^4.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "redux-mock-store": "^1.5.4",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "4.3.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precomit"
    }
  }
}
